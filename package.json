{
  "name": "FuBoFleaMarket",
  "version": "1.0.0",
  "description": "A flea market web site for fu-bo",
  "main": "app/app.js",
  "scripts": {
    "build": "better-npm-run build",
    "lint-staged": "lint-staged",
    "eslint:fix": "eslint --fix",
    "eslint": "eslint --fix --ext .js app test",
    "git:add": "git add",
    "start": "if-env NODE_ENV=production && npm run prod || npm run dev",
    "watch-client": "better-npm-run watch-client",
    "start-dev": "better-npm-run start-dev",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "start-prod": "better-npm-run start-prod",
    "prod": "concurrently --kill-others \"npm run start-prod\"",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/setup.js --reporter mochawesome",
    "test:watch": "npm test -- --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production"
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./app",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack.prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Fu-Bo-Tech/fubo-flea-market.git"
  },
  "author": "ben196888, ElaineHuang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Fu-Bo-Tech/fubo-flea-market/issues"
  },
  "homepage": "https://github.com/Fu-Bo-Tech/fubo-flea-market#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.5",
    "better-npm-run": "0.0.11",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.1",
    "lint-staged": "^2.0.3",
    "mocha": "^2.5.3",
    "mochawesome": "^1.4.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.8.2",
    "postcss-nested": "^1.0.0",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^1.3.0",
